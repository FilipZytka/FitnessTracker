<h2>Exercise Log Creation</h2>

<hr />

<div class="row">
    <div class=" col-md-4"></div>
    <form action="/FitnessTracker/Create" method="post">
        <div class="form-group mb-3">
            <label class="control-label" for="Name">Name</label>
            <input class="form-control" type="text" id="Name" name="Name"/>
        </div>

        <div class="form-group mb-3">
            <label class="control-label" for="Description">Description</label>
            <input class="form-control" type="text" id="Description" name="Description" />
        </div>

        <div class="form-group mb-3">
            <label class="control-label" for="Sets">Sets</label>
            <input class="form-control" type="text" id="Sets" name="Sets" />
        </div>

        <div class="form-group mb-3">
            <label class="control-label" for="Reps">Reps</label>
            <input class="form-control" type="text" id="Reps" name="Reps" />
        </div>

        <div class="form-group mb-3">
            <input type="submit" value="Create" class="btn btn-light" />
        </div>
    </form>

</div>